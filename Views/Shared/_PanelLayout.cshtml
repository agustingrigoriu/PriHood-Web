<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <base href="/">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Panel - @ViewData["Title"] - PriHood</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="panel/bootstrap/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="panel/css/AdminLTE.min.css">
  <link rel="stylesheet" href="panel/css/skins/skin-blue.min.css">
  <link rel="stylesheet" href="panel/css/panel.css">
  <!-- <link rel="stylesheet" href="panel/css/tabulator.min.css"> -->
  <link href="/panel/css/tabulator_bootstrap.min.css" rel="stylesheet">
  <script src="~/panel/plugins/jQuery/jquery-2.2.3.min.js"></script>
</head>

<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  @Html.Partial("_NavBar")

  @Html.Partial("_SideBar")

  @RenderBody()

  @Html.Partial("_Footer")

</div>
<!-- ./wrapper -->


<script src="~/panel/plugins/jQueryUI/jquery-ui.min.js"></script>
<script src="~/panel/plugins/tabulator/tabulator.min.js"></script>
<script src="~/panel/bootstrap/js/bootstrap.min.js"></script>
<script src="~/panel/js/app.min.js"></script>

</body>
</html>
